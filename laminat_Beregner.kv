<WindowManager>:
    MenuScreen:
    LaminatBeregner:

<TooltipMDRectangleFlatIconButton@MDRectangleFlatIconButton+MDTooltip>

<MenuScreen>:
    name: 'menu'

    MDFloatLayout:
        size: root.width, root.height
        


        MDLabel:
            font_name: 'Font/consolaz.ttf'
            font_size: '20sp'
            text: "Welcome To An EmberFiles Product" 
            pos_hint: {"x": .30, "y": .45}
        
        TooltipMDRectangleFlatIconButton:
            icon: 'icon/LB.png'
            icon_size: 30
            tooltip_text: "hey ho, let's go!"
            tooltip_bg_color: 173,173,173,0.50
            tooltip_text_color: 0,0,0,1
            text_size: self.size
            padding: ["80dp", "10dp", "80dp", "10dp"]
            halign: 'right'
            line_color: "red"
            text_color: "white"
            text: "Laminat/Gulv Beregner"
            pos_hint: {"x": .1, "y": .7}

<LaminatBeregner>:
    name: 'main'


    

